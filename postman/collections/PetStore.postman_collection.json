      "name": "01  - POST Add Pet",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "const json = pm.response.json();\r",
              "\r",
              "// Guardar variables\r",
              "pm.environment.set(\"petId\", json.id);\r",
              "pm.environment.set(\"petName\", json.name);\r",
              "\r",
              "pm.test(\"petId fue generado\", function () {\r",
              "    pm.expect(json.id).to.be.a(\"number\");\r",
              "});\r",
              "\r",
              "pm.test(\"petName no viene vac√≠o\", function () {\r",
              "    pm.expect(json.name).to.be.a(\"string\").and.not.empty;\r",
              "});\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
