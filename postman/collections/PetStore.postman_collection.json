    {
      "name": "03 - Update pet to sold",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "const json = pm.response.json();\r",
              "\r",
              "// Validar que el id sea el mismo\r",
              "pm.test(\"El id se mantiene\", function () {\r",
              "    pm.expect(json.id).to.eql(Number(pm.environment.get(\"petId\")));\r",
              "});\r",
              "\r",
              "// Validar que el nombre fue actualizado\r",
              "pm.test(\"El nombre fue actualizado\", function () {\r",
              "    pm.expect(json.name).to.eql(\"SofkaPetSold\");\r",
              "});\r",
              "\r",
              "// Validar que el status es sold\r",
              "pm.test(\"El status es sold\", function () {\r",
              "    pm.expect(json.status).to.eql(\"sold\");\r",
              "});\r",
              ""
